{
  "name": "liquidator",
  "version": "1.0.0",
  "scripts": {
    "build": "esbuild index.ts --bundle --platform=node --target=node22 --outfile=dist/index.js '--alias:common-utils=../../packages/common-utils/src/index.ts' '--external:@aws-sdk/*' '--external:@radixdlt/*' '--external:@weft-finance/*' --external:pino",
    "test": "pnpm run build && bun test"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.450.0",
    "@aws-sdk/client-sqs": "^3.450.0",
    "@radixdlt/babylon-gateway-api-sdk": "^1.0.0",
    "@weft-finance/ledger-state": "^0.0.16",
    "common-utils": "workspace:*",
    "pino": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "esbuild": "^0.24.0",
    "typescript": "^5.0.0"
  }
}